@using ParksClient.Models;
@using Microsoft.AspNetCore.Mvc.Rendering; 
@model Park;

@using (Html.BeginForm())
{
    @Html.Label("Park", "Park: ")
    @Html.TextBoxFor(model => model.Name)

    @Html.Label("Type", "Type(State or National): ")
    @Html.TextBoxFor(model => model.Land)

    @Html.Label("State", "State Initials: ")
    @Html.TextBoxFor(model => model.State)

<button type="submit" class="btn btn-info">Add Park</button>

}

<button class="btn btn-default">@Html.ActionLink("Back To Home", "Index", new {style = "color:white"})</button>